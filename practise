//2.
class Case_Sensitive{
    public static void main (String[] args){
        int Variable = 10;
        int variable = 30;
        System.out.println("cs "+ Variable);
        System.out.println("cs "+ variable);
    }
}
//3.
import java.util.*;
class Main {
    public static void main (String[] args){
        int a, b ;
        Scanner r = new Scanner(System.in);
        a= r.nextInt();
        b= r.nextInt();
        System.out.println(a+b);
        System.out.println(a-b);
        System.out.println(a*b);
        System.out.println(a/b);
    }
}
